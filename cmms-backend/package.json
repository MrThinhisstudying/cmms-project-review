{
    "name": "acv",
    "private": true,
    "version": "0.0.1",
    "description": "Simple experiment with IPFS",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:docker": "docker-compose up --build",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:generate": "typeorm migration:generate src/migrations/NewMigration -d src/data-source.ts",
        "migration:run": "typeorm migration:run -d src/data-source.ts",
        "migration:revert": "typeorm migration:revert -d src/data-source.ts",
        "gen:subgraph": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/axios": "^0.0.7",
        "@nestjs/common": "^8.4.4",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.4.4",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.4.4",
        "@nestjs/platform-socket.io": "^8.4.7",
        "@nestjs/schedule": "^6.1.0",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/throttler": "^2.0.1",
        "@nestjs/typeorm": "^8.0.3",
        "@nestjs/websockets": "^8.4.7",
        "@types/cache-manager": "^4.0.0",
        "@types/pdfmake": "^0.2.12",
        "bcrypt": "^5.0.1",
        "cache-manager": "^4.1.0",
        "chrome-aws-lambda": "^10.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.8.0",
        "connect-pg-simple": "^10.0.0",
        "cookie-parser": "^1.4.7",
        "dayjs": "^1.11.18",
        "docx": "^9.5.1",
        "docxtemplater": "^3.67.6",
        "docxtemplater-image-module-free": "^1.1.1",
        "dotenv": "^16.0.0",
        "exceljs": "^4.4.0",
        "express": "^4.18.1",
        "express-session": "^1.17.2",
        "factory.ts": "^1.1.0",
        "multer": "^1.4.5-lts.1",
        "nestjs-cls": "^6.0.1",
        "nodemailer": "^7.0.6",
        "passport": "^0.5.2",
        "passport-custom": "^1.1.1",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pdf-lib": "^1.17.1",
        "pdfmake": "^0.3.2",
        "pg": "^8.15.6",
        "pizzip": "^3.2.0",
        "puppeteer": "^24.31.0",
        "puppeteer-core": "^24.31.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "socket.io": "^4.8.3",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.27",
        "uuid": "^8.3.2",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.2.5",
        "@nestjs/schematics": "^8.0.10",
        "@nestjs/testing": "^8.4.4",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/connect-pg-simple": "^7.0.3",
        "@types/cookie-parser": "^1.4.8",
        "@types/cron": "^2.4.3",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.18.1",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.1",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.29",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.12",
        "@types/xlsx": "^0.0.36",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "faker": "^5.5.3",
        "jest": "^27.1.4",
        "jest-mock-extended": "^2.0.6",
        "mockdate": "^3.0.5",
        "prettier": "^2.6.2",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.9",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
    }
}
